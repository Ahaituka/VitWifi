<Page
    x:Class="Volsbb_Onetouch.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Volsbb_Onetouch"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

        <TextBlock  x:Name="User"  Text="Unregistered"/>
        <TextBlock  x:Name="Pass"  Text="Unregistered"/>       
        

        <Button HorizontalAlignment="Center" Height="50" Width="150" Click="Button_Click" >Register</Button>
        <Button HorizontalAlignment="Right"    Height="50" Width="150" Margin="150" Click="Button_Click_1" >Unregister </Button>
        
        

       



    </Grid>
</Page>
